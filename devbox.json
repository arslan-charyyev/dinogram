{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.14.2/.schema/devbox.schema.json",
  "packages": [
    "deno@2.2.12",
    "x11vnc@0.9.17"
  ],
  "env": {
    "COMPOSE_BAKE": "true",
    "DENO_NO_UPDATE_CHECK": "1"
  },
  "shell": {
    "init_hook": "deno task deps:cache",
    "scripts": {
      "docker": "docker compose up --build"
    }
  }
}
