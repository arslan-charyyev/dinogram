{
  "deno.enable": true,
  "editor.formatOnSave": true,
  "deno.path": ".devbox/nix/profile/default/bin/deno",
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.tabSize": 2,
  "cSpell.files": ["./**/*"],
  "cSpell.words": [
    "abtest",
    "acrawler",
    "arslan",
    "byted",
    "charyyev",
    "cookiejar",
    "csrftoken",
    "datetime",
    "deno",
    "denoland",
    "devbox",
    "Dinogram",
    "grammyjs",
    "jetify",
    "ratelimiter",
    "Shiba",
    "shortcode",
    "Stringable",
    "Tele",
    "tiktok",
    "Vibing",
    "webid",
    "webmssdk"
  ],
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
