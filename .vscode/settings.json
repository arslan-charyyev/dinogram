{
  "deno.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.tabSize": 2,
  "cSpell.files": ["./**/*"],
  "cSpell.words": [
    "abtest",
    "acrawler",
    "aiogram",
    "arslan",
    "asyncutil",
    "byted",
    "charyyev",
    "cookiejar",
    "csrftoken",
    "datetime",
    "deno",
    "denoland",
    "Dinogram",
    "grammyjs",
    "jetify",
    "memoizee",
    "ncache",
    "ratelimiter",
    "rfbport",
    "Shiba",
    "shortcode",
    "Stringable",
    "Tele",
    "tiktok",
    "Vibing",
    "webid",
    "webmssdk"
  ],
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-containers"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[html]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "search.exclude": {
    "assets/js/**": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
