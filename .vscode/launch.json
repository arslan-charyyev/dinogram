{
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "main",
      "type": "node",
      "program": "src/main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "outputCapture": "std",
      "runtimeArgs": [
        "run",
        "--allow-all",
        "--inspect-wait"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "test current",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "${file}",
        "--allow-all",
        "--inspect-wait"
      ],
      "attachSimplePort": 9229
    }
  ]
}
